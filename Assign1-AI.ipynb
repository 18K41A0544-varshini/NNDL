    "it=0\n",
    "max_it=100\n",
    "n=0.01\n",
    "x=5\n",
    "deltax=1\n",
    "\n",
    "\n",
    "\n",
    "def der(x):\n",
    "    x_der=(4*(x*x*x))+(6*x)\n",
    "    return x_der\n",
    "\n",
    "while it<max_it:\n",
    "    x1=x;\n",
    "    deltax=-n*der(x1)\n",
    "    x=x+deltax\n",
    "    it=it+1\n",
    "    print(\"iteration\",it)\n",
    "    print(\"value is\",x)\n",
    "    \n",
    "    \n",
    "    \n",
    "\n",
    "print(\"local minimum point\",x)"
